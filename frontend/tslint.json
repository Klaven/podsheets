{
  "extends": "tslint:latest",
  "rules": {
    "curly": true,
    "arrow-parens": false,
    "only-arrow-functions": [false],
    "object-literal-sort-keys": false,
    "variable-name": [true, "ban-keywords", "allow-leading-underscore"],
    "space-before-function-paren": false,
    "max-line-length": [true, 240]
  },
  "jsRules": { "curly": true },
  "rulesDirectory": []
}